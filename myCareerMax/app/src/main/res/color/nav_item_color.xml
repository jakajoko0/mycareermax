<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Color when the navigation item is selected -->
    <item android:color="#12FF00" android:state_checked="true"/>
    <!-- Color when the navigation item is pressed -->
    <item android:color="#FFE500" android:state_pressed="true"/>
    <!-- Default color of the navigation item -->
    <item android:color="#FFFFFF"/>
</selector>
